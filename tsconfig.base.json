{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@racoon/racoon/about-us/smart": [
        "libs/racoon/about-us/smart/src/index.ts"
      ],
      "@racoon/racoon/about-us/ui": ["libs/racoon/about-us/ui/src/index.ts"],
      "@racoon/racoon/check-out/smart": [
        "libs/racoon/check-out/smart/src/index.ts"
      ],
      "@racoon/racoon/check-out/ui": ["libs/racoon/check-out/ui/src/index.ts"],
      "@racoon/racoon/home/smart": ["libs/racoon/home/smart/src/index.ts"],
      "@racoon/racoon/home/ui": ["libs/racoon/home/ui/src/index.ts"],
      "@racoon/racoon/log-in/smart": ["libs/racoon/log-in/smart/src/index.ts"],
      "@racoon/racoon/log-in/ui": ["libs/racoon/log-in/ui/src/index.ts"],
      "@racoon/racoon/payment/smart": [
        "libs/racoon/payment/smart/src/index.ts"
      ],
      "@racoon/racoon/products/smart": [
        "libs/racoon/products/smart/src/index.ts"
      ],
      "@racoon/racoon/products/ui": ["libs/racoon/products/ui/src/index.ts"],
      "@racoon/racoon/shared": ["libs/racoon/shared/src/index.ts"],
      "@racoon/racoon/sign-up/smart": [
        "libs/racoon/sign-up/smart/src/index.ts"
      ],
      "@racoon/racoon/sign-up/ui": ["libs/racoon/sign-up/ui/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
